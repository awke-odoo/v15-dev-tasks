<?xml version="1.0" encoding="utf-8"?>
<odoo>
  <record id="view_sale_subscription_form_custom" model="ir.ui.view">
    <field name="name">sale.subscription.form.custom</field>
    <field name="model">sale.subscription</field>
    <field
      name="inherit_id"
      ref="sale_subscription.sale_subscription_view_form"
    />
    <field name="arch" type="xml">
      <xpath expr="//field[@name='partner_id']" position="after">
        <field name="property_partner_id" widget="many2one" />
      </xpath>
    </field>
  </record>
</odoo>
